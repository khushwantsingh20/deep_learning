{
  "name": "southern_cross",
  "version": "0.0.0",
  "description": "Southern Cross Booking Platform",
  "private": true,
  "main": "bundle.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "cd frontend && yarn run clean && NODE_ENV=production webpack",
    "postbuild": "mkdir -p frontend/dist-source-maps && mv frontend/dist/*.map frontend/dist-source-maps/ || :",
    "clean": "rm -rf frontend/dist && rm -rf frontend/dist-source-map",
    "dev": "cd frontend && node webpack.devserver.js",
    "lint": "cd frontend && LINT_PRETTIER=1 eslint src cypress",
    "lintfix": "cd frontend && LINT_PRETTIER=1 eslint src cypress --fix",
    "test": "jest",
    "test-e2e": "cypress open",
    "test-e2e:headless": "cypress run",
    "styleguide": "styleguidist server --config frontend/styleguide/styleguide.config.js"
  },
  "author": "Alliance Software <support@alliancesoftware.com.au> (http://www.alliancesoftware.com.au)",
