{
  "name": "alliance-react",
  "version": "1.1.0",
  "description": "A collection of reusable React components",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/alliance-react.js && NODE_ENV=production webpack src/index.js dist/alliance-react.min.js",
    "lint": "eslint src test examples",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alliance-react-author/alliance-react.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Dave Coates",
  "license": "MIT",
  "bugs": {
    "url": "http://gitlab.internal.alliancesoftware.com.au/alliance/alliance-react/merge_requests"
  },
  "homepage": "http://gitlab.internal.alliancesoftware.com.au/alliance/alliance-react",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.4",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "enzyme": "^2.5.1",
    "eslint": "^3.2.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-alliance": "git+ssh://git@gitlab.internal.alliancesoftware.com.au:alliance/eslint-config.git#v1.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^5.2.2",
    "jest": "^16.0.2",
    "react-addons-test-utils": "^0.14.7",
    "react-test-renderer": "^15.3.2",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "invariant": "^2.0.0",
    "prop-types": "^15.5.10",
    "react-side-effect": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || >=16"
  }
}
